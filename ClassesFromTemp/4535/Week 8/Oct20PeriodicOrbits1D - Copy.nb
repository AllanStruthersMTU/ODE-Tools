Cell[TextData[{
 "If",
 Cell[BoxData[
  FormBox[
   RowBox[{"|", 
    RowBox[{
     SubscriptBox["p", "n"], "-", 
     SubscriptBox["p", 
      RowBox[{"n", "+", "1"}]]}], "|", 
    RowBox[{"\[LessEqual]", 
     SuperscriptBox["L", "n"]}], "|", 
    RowBox[{
     SubscriptBox["p", "0"], "-", 
     SubscriptBox["p", "1"]}], "|"}], TraditionalForm]], "Subitem",
  ExpressionUUID->"90287bbb-c197-4580-8933-6530b3c71f0e"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "\[LessEqual]", "L", "<", "1"}], TraditionalForm]],
  ExpressionUUID->"fba9a13c-54f7-47cc-bca4-022bafb220db"],
 " then",
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"|", 
     RowBox[{
      SubscriptBox["p", "n"], "-", 
      SubscriptBox["p", 
       RowBox[{"n", "+", "1"}]]}], "|"}], " ", "->", "0"}], TraditionalForm]],
  ExpressionUUID->"7779cc95-de6b-4f83-9cd5-7b899465b606"],
 " and since ",
 Cell[BoxData[
  FormBox["\[DoubleStruckCapitalR]", TraditionalForm]],ExpressionUUID->
  "451654af-15f6-4560-b84e-051c42478ce4"],
 " has no holes (fancy way to say this is \[DoubleStruckCapitalR] is \
complete) this Cauchy sequence converges to something i.e. ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["p", "n"], "->", 
    SubscriptBox["p", "*"]}], TraditionalForm]],ExpressionUUID->
  "686967d4-0749-4fd6-acd1-069213c2eb4e"],
 " which ",
 Cell[BoxData[
