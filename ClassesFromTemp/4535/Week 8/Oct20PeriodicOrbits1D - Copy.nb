Cell[CellGroupData[{

Cell[TextData[{
 "If g makes things closer together in the sense that ",
 Cell[BoxData[
  FormBox[
   RowBox[{"|", 
    RowBox[{
     RowBox[{"g", "[", 
      SubscriptBox["x", "1"], "]"}], "-", 
     RowBox[{"g", "[", 
      SubscriptBox["x", "2"], "]"}]}], "|", 
    RowBox[{"\[LessEqual]", " ", "L"}], "|", 
    RowBox[{
     SubscriptBox["x", "1"], "-", 
     SubscriptBox["x", "2"]}], "|"}], TraditionalForm]],ExpressionUUID->
  "5e8232df-92a6-4662-a89a-0eaded6c722c"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"L", "<", "1"}], TraditionalForm]],ExpressionUUID->
  "967e8216-40b9-4236-b2b8-c614e8e7e31e"],
 " then \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"|", 
     RowBox[{
      SubscriptBox["p", "n"], "-", 
      SubscriptBox["p", 
       RowBox[{"n", "+", "1"}]]}], "|"}], "=", 
    RowBox[{
     RowBox[{"|", 
      RowBox[{
       RowBox[{"g", "[", 
        SubscriptBox["p", 
         RowBox[{"n", "-", "1"}]], "]"}], "-", 
       RowBox[{"g", "[", 
        SubscriptBox["p", "n"], "]"}]}], "|", 
      RowBox[{"\[LessEqual]", "L"}], "|", 
      RowBox[{
       SubscriptBox["p", 
        RowBox[{"n", "-", "1"}]], "-", 
       SubscriptBox["p", "n"]}], "|"}], "=", 
     RowBox[{"L", "|", 
      RowBox[{
       RowBox[{"g", "[", 
        SubscriptBox["p", 
         RowBox[{"n", "-", "2"}]], "]"}], "-", 
       RowBox[{"g", "[", 
        SubscriptBox["p", 
         RowBox[{"n", "-", "1"}]], "]"}]}], "|", 
      RowBox[{"\[LessEqual]", 
       SuperscriptBox["L", "2"]}], "|", 
      RowBox[{
       SubscriptBox["p", 
        RowBox[{"n", "-", "2"}]], "-", 
       SubscriptBox["p", 
        RowBox[{"n", "-", "1"}]]}], "|"}]}]}], TraditionalForm]],
  ExpressionUUID->"a7c75ff7-554f-4c1a-9352-c0dfec31df00"],
 "\nContinuing this gives. \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"|", 
    RowBox[{
     SubscriptBox["p", "n"], "-", 
     SubscriptBox["p", 
      RowBox[{"n", "+", "1"}]]}], "|", 
    RowBox[{"\[LessEqual]", 
     SuperscriptBox["L", "n"]}], "|", 
    RowBox[{
     SubscriptBox["p", "0"], "-", 
     SubscriptBox["p", "1"]}], "|"}], TraditionalForm]], "Subitem",
  ExpressionUUID->"13e34987-2b78-4dd9-a265-e558e5494a91"]
}], "Item",
