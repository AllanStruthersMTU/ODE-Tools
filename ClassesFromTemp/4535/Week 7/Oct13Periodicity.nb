Cell[CellGroupData[{

Cell["Derivatives and analysis for period two!", "Subsection",
 CellChangeTimes->{{3.8430430699900656`*^9, 3.843043073456728*^9}, {
  3.8430443923326545`*^9, 3.8430444041526484`*^9}, {3.8430472804765654`*^9, 
  3.8430472810557694`*^9}},ExpressionUUID->"2b5b12ac-31a1-401a-bc3a-\
b79f5dda484d"],

Cell[TextData[{
 "A period two orbit  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     SubscriptBox["p", "1"], ",", 
     SubscriptBox["p", "2"]}], "}"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a4efb641-c0c5-4f43-a0f6-8cae05ac89f3"],
 " of ",
 Cell[BoxData[
  FormBox["g", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "5aecbbe7-2529-4302-a5b4-0aa55c0e641b"],
 " satisfies ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["p", "2"], "=", 
    RowBox[{"g", "(", 
     SubscriptBox["p", "1"], ")"}]}], TraditionalForm]],
  InitializationCell->True,
  FormatType->TraditionalForm,ExpressionUUID->
  "c165cea9-32a9-4c06-b2e7-82c15eb107a6"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["p", "1"], "=", 
    RowBox[{"g", "(", 
     SubscriptBox["p", "2"], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "95d1ec4c-74cf-4f63-9631-9b2936fa5567"],
 ".  Period two orbits show up as a pair of values which satisfy\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["p", "1"], "=", 
    RowBox[{"g", "(", 
     RowBox[{"g", "(", "p1", ")"}], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "34c98ced-15ab-4470-96a9-8d72b0ebf1a9"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["p", "2"], "=", 
    RowBox[{"g", "(", 
     RowBox[{"g", "(", 
      SubscriptBox["p", "2"], ")"}], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "6ffa5c8c-2884-4b81-b16e-ad3be960b251"],
 " \nIn other words they are twin fixed points of ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["g", 
    RowBox[{"\[SmallCircle]", "2"}]], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e2400113-d928-4ca4-8835-f6957bcd64de"],
 ".  Differentiating ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"g", "(", 
     RowBox[{"g", "(", 
      SubscriptBox["p", "1"], ")"}], ")"}], " "}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "c306ce80-c6da-4118-aefb-bf2dcc9206ae"],
 "we get \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"D", "(", 
    RowBox[{
     RowBox[{"g", "(", 
      RowBox[{"g", "(", 
       SubscriptBox["p", "1"], ")"}], ")"}], "=", " ", 
     RowBox[{"Dg", "(", 
      RowBox[{
       RowBox[{
        RowBox[{"g", "(", 
         SubscriptBox["p", "1"], ")"}], ".", 
        RowBox[{"Dg", "(", 
         SubscriptBox["p", "1"], ")"}]}], "=", 
       RowBox[{
        RowBox[{"Dg", "(", 
         SubscriptBox["p", "2"], ")"}], ".", 
        RowBox[{"Dg", "(", 
         SubscriptBox["p", "1"], ")"}]}]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "308ca000-240a-44c5-b059-991b517da849"],
 "\nIf we write ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["A", "1"], "=", 
    RowBox[{"Dg", "(", 
     SubscriptBox["p", "1"], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "0e057a60-841c-49ea-87fb-72c2861ed711"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["A", "2"], "=", 
    RowBox[{"Dg", "(", 
     SubscriptBox["p", "1"], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "9e422ef1-a77e-4fa2-b35e-818aa06d8fb1"],
 " then we can write\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"D", "(", 
    RowBox[{
     RowBox[{"g", "(", 
      RowBox[{"g", "(", 
       SubscriptBox["p", "1"], ")"}], ")"}], "=", " ", 
     RowBox[{
      SubscriptBox["A", "2"], ".", 
      SubscriptBox["A", "1"]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7f1484c4-09b0-4281-a05e-8c852e813f65"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"D", "(", 
    RowBox[{
     RowBox[{"g", "(", 
      RowBox[{"g", "(", 
       SubscriptBox["p", "2"], ")"}], ")"}], "=", " ", 
     RowBox[{
      SubscriptBox["A", "1"], ".", 
      SubscriptBox["A", "2"]}]}]}], TraditionalForm]],ExpressionUUID->
  "c412fc1e-62a6-4b4d-9089-3b9eb9a39545"],
 Cell[BoxData[
  FormBox[" ", TraditionalForm]],ExpressionUUID->
  "58edaac2-ccb3-4821-90a2-7f764511b284"],
 "\nThe period two pair is stable if all the eigenvalues of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["A", "2"], 
    SubscriptBox["A", "1"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "5d980c64-2c8d-40dd-b35f-dc500580e64c"],
 " have magnitude less than one. "
}], "Text",
 CellChangeTimes->{{3.8430430759634886`*^9, 3.8430434614588118`*^9}, {
   3.84304358505744*^9, 3.8430436489780273`*^9}, {3.8430439245964456`*^9, 
   3.843043927171402*^9}, {3.8430441731883993`*^9, 3.8430441761916924`*^9}, {
   3.8430445807439327`*^9, 3.8430448671513367`*^9}, 3.8430449106700563`*^9, {
   3.843047295944685*^9, 3.8430474916148443`*^9}, {3.8430475752073565`*^9, 
   3.84304780707141*^9}, {3.8430482330121727`*^9, 3.84304844099692*^9}, {
   3.8430486436007595`*^9, 
   3.8430486528950033`*^9}},ExpressionUUID->"9888b531-dd8b-4f32-b539-\
99b3bfcad614"],

Cell["\<\
We found two pairs of period two orbits in our g up above: The other two are \
the fixed points in p1.\
\>", "Text",
 CellChangeTimes->{{3.843048657067135*^9, 
  3.843048736287472*^9}},ExpressionUUID->"a5f2b1b3-9925-4cde-a983-\
fff99934d33c"],

Cell[BoxData[
 RowBox[{"p2Alone", " ", "=", 
  RowBox[{"Complement", "[", 
   RowBox[{"p2", ",", "p1", ",", 
    RowBox[{"SameTest", ":>", 
     RowBox[{"(", 
      RowBox[{
